{
  "name": "ace",
  "productName": "ace",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "lint-fix": "npm run lint -- --fix",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "FlyByWire Simulations, Synaptic Simulations",
  "license": "GPLv3",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ace"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            },
            "devContentSecurityPolicy": "*"
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "@electron-forge/plugin-webpack": "6.0.0-beta.59",
    "@flybywiresim/eslint-config": "^0.2.2",
    "@flybywiresim/tailwind-config": "^0.5.2",
    "@types/cors": "^2.8.12",
    "@types/discord-rpc": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.6.0",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.0.0",
    "electron": "^13.2.3",
    "electron-devtools-installer": "github:MarshallOfSound/electron-devtools-installer",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "react-zoom-pan-pinch": "^2.1.3",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "tailwindcss": "^2.2.9",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@flybywiresim/react-components": "^0.3.2",
    "@reduxjs/toolkit": "^1.6.1",
    "@tabler/icons": "^1.41.2",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "highlight.js": "^11.4.0",
    "http-proxy-middleware": "^2.0.4",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react-searchable-filter": "^1.1.0",
    "react-use": "^17.3.1",
    "react-windows-controls": "^1.1.1",
    "redux": "^4.1.1",
    "use-events": "^1.4.2",
    "uuid": "^8.3.2"
  }
}
